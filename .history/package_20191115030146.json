  "homepage": "https://github.com/sillasleal/js-extras#readme",
  "author": "Sillas S. Leal<sillas.s.leal@gmail.com>",
  "license": "MIT",
  "scripts": {
    "preinstall": "node -e \"const fs = require('fs');if(fs.existsSync('./package-lock.json')){fs.unlinkSync('./package-lock.json');}\"",
    "prebuild": "node -e \"const fs = require('fs-extra');fs.removeSync('./dist');\"",
    "build": "babel ./src --out-dir ./dist",
    "test:bk": "nyc npm run test:dev && nyc --reporter=html --reporter=text --reporter=json --reporter=text-lcov | coveralls",
    "test": "nyc npm test:dev && nyc report --reporter=text-lcov",
    "test:dev": "mocha test/**/*.js",
    "test:watch": "nodemon --exec npm run test",
    "predeploy": "yarn run build && yarn run eslint:fix",
    "prepublish": "yarn run test",
    "deploy": "npm publish --access=public",
    "postdeploy": "git add . && git commit && git push",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint ./src --fix"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "@babel/runtime": "7.7.2",
    "chai": "4.2.0",
    "coveralls": "3.0.7",
    "eslint": "6.1.0",
    "eslint-config-google": "0.13.0",
    "fs-extra": "7.0.1",
    "mocha": "6.0.2",
    "mocha-lcov-reporter": "1.3.0",
    "nodemon": "1.18.10",
    "nyc": "13.3.0"
  }
}
