  "description": "",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sillasleal/js-extras.git"
  },
  "bugs": {
    "url": "https://github.com/sillasleal/js-extras/issues"
